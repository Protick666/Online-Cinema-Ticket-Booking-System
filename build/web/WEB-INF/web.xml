    <servlet>
        <servlet-name>Tickets</servlet-name>
        <servlet-class>ch01.Tickets</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>E_tickets</servlet-name>
        <servlet-class>ch01.E_tickets</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>E_Logout</servlet-name>
        <servlet-class>ch01.E_Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>ch01.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>myservlet</servlet-name>
        <servlet-class>ch01.myservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mysecondservlet</servlet-name>
        <url-pattern>/mysecondservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmployeeServlet</servlet-name>
        <url-pattern>/EmployeeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Movies</servlet-name>
        <url-pattern>/Movies</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Movie_authorize</servlet-name>
        <url-pattern>/Movie_authorize</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Booking</servlet-name>
        <url-pattern>/Booking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tickets</servlet-name>
        <url-pattern>/Tickets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>E_tickets</servlet-name>
        <url-pattern>/E_tickets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>E_Logout</servlet-name>
        <url-pattern>/E_Logout</url-pattern>
    </servlet-mapping>
